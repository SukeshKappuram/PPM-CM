    "build-uat": "node build-version.js uat && ng build --configuration uat",
    "build-qa": "node build-version.js qa && ng build --configuration qa",
    "build-blue": "node build-version.js blue && ng build --configuration blue",
    "build-dev": "node build-version.js dev && ng build --configuration dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npx eslint \"src/**/*.{ts,html,css}\" --quiet --fix",
    "format": "npx prettier \"src/**/*.{ts,html,css}\" --write",
